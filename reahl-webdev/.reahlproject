<project type="egg" packagedata="included">
  <namespaces>
    <package name="reahl"/>
  </namespaces>

  <deps purpose="run">
    <egg name="reahl-web"/>
    <egg name="reahl-dev"/>
    <egg name="reahl-component"/>
    <egg name="reahl-tofu"/>
    
    <thirdpartyegg name="lxml" minversion="3.3" maxversion="3.3.999"/>
    <thirdpartyegg name="WebTest" minversion="1.4" maxversion="1.5"/>
    <thirdpartyegg name="selenium" minversion="2.42" maxversion="2.42.999"/>
  </deps>

  <extras name="pillow">
    <thirdpartyegg name="pillow" minversion="2.5" maxversion="2.5.999"/>
  </extras>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    <egg name="reahl-stubble"/>
  </deps>

  <export entrypoint="reahl.dev.commands" name="ServeCurrentProject" locator="reahl.webdev.commands:ServeCurrentProject"/>
  
  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture"/>
  
  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>

</project>


