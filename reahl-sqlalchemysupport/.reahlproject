<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>
  
  <configuration locator="reahl.sqlalchemysupport:SqlAlchemyConfig"/>

  <deps purpose="run">
    <egg name="reahl-component"/>
    <thirdpartyegg name="sqlalchemy" minversion="0.7" maxversion="0.7.999"/>
    <thirdpartyegg name="alembic" minversion="0.5" maxversion="0.5.999"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    <egg name="reahl-stubble"/>
  </deps>

  <persisted>
    <class locator="reahl.sqlalchemysupport:SchemaVersion"/>
  </persisted>

  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture -T reahl/sqlalchemysupport_dev -F reahl.dev.fixtures:CleanDatabase"/>

  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>

</project>
