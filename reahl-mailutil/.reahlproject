<project type="egg">
  <configuration locator="reahl.mailutil.reusableconfig:MailConfig"/>
  <namespaces>
    <package name="reahl"/>
  </namespaces>
  
  <deps purpose="run">
    <egg name="reahl-component"/>
    <thirdpartyegg name="docutils" minversion="0.12" maxversion="0.12"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    
    <egg name="reahl-stubble"/>
  </deps>

  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture -T reahl/mailutil_dev"/>
  
  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>
  
</project>
