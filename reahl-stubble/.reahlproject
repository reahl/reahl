<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>

  <deps purpose="test">
    <thirdpartyegg name="nose"/>
    <egg name="reahl-tofu"/>
  </deps>

  <deps purpose="build">
    <egg name="reahl-bzrsupport"/>
  </deps>

  <alias name="unit" command="setup -- -q tofu"/> 
  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture -T reahl/stubble_dev"/>
  
  <distpackage type="sdist">
    <packageindex repository="devpi"/>
  </distpackage>

</project>
