<project type="egg">
  <deps purpose="run">
    <egg name="reahl-web"/>
    <egg name="reahl-component"/>
    <egg name="reahl-web-declarative"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    <egg name="reahl-stubble"/>
    <egg name="reahl-dev"/>
    <egg name="reahl-webdev"/>
  </deps>

  <persisted>
    <class locator="reahl.doc.examples.features.fileupload.fileupload:Comment"/>
    <class locator="reahl.doc.examples.features.fileupload.fileupload:AttachedFile"/>
  </persisted>

  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture --tests=fileupload_dev/ -t -i '.*'"/>

</project>
