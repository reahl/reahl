[metadata]
name = migrationexamplebootstrap
version = 0.1

[options]
py_modules =
  migrationexamplebootstrap

install_requires =
  reahl>=6.0,<6.1
  reahl-web
  reahl-component
  reahl-sqlalchemysupport
  reahl-sqlitesupport
  reahl-web-declarative

tests_require =
  pytest>=3.0
  reahl-tofu
  reahl-stubble
  reahl-dev
  reahl-webdev
  reahl-browsertools

component = 
  persisted = [
    "reahl.doc.examples.tutorial.migrationexamplebootstrap.migrationexamplebootstrap:Address"
  ]
  # List all major.minor versions:
  [versions."0.1"]
    migrations = [
      "reahl.doc.examples.tutorial.migrationexamplebootstrap.migrationexamplebootstrap:AddDate"
    ]
  [versions."0.0"]
    install_requires = [
      "reahl>=6.0,<6.1",
      "reahl-web>=6.0,<6.1",
      "reahl-component>=6.0,<6.1",
      "reahl-sqlalchemysupport>=6.0,<6.1",
      "reahl-sqlitesupport>=6.0,<6.1",
      "reahl-web-declarative>=6.0,<6.1"
    ]





