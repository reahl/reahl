<project type="egg">
  <deps purpose="run">
    <egg name="reahl-web"/>
    <egg name="reahl-component"/>
    <egg name="reahl-sqlalchemysupport"/>
    <egg name="reahl-web-declarative"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    <egg name="reahl-stubble"/>
    <egg name="reahl-dev"/>
    <egg name="reahl-webdev"/>
  </deps>

  <persisted>
    <class locator="reahl.doc.examples.tutorial.tablebootstrap.tablebootstrap:Address"/>
  </persisted>
  
  <alias name="unit" command="setup -- -q nosetests -s --with-id --nologcapture --tests=tablebootstrap_dev -t -i '.*'"/>
  <alias name="demosetup" command="setup -- -q nosetests -F reahl.webdev.fixtures:BrowserSetup --with-setup-fixture=reahl.doc.examples.tutorial.tablebootstrap.tablebootstrap_dev.tablebootstraptests:DemoSetup" />

</project>
