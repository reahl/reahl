<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>
  
  <version number="6.1">
    <deps purpose="run">
      <egg name="reahl-component" versionlocked="True"/>
      <egg name="reahl-tofu" versionlocked="True"/>
      <egg name="reahl-stubble" versionlocked="True"/>
      <egg name="reahl-sqlalchemysupport" versionlocked="True"/>
      <thirdpartyegg name="Babel" minversion="2.1" maxversion="2.11.999"/>
      <thirdpartyegg name="Pygments" minversion="2.1.0" maxversion="2.13.999"/><!-- because twine depends on Pygments that conflict with ours -->
      <thirdpartyegg name="twine" minversion="1.15.0" maxversion="4.0.9999"/>
      <thirdpartyegg name="wheel" minversion="0.34.0"/>
      <thirdpartyegg name="tzlocal" minversion="2.0.0" maxversion="4.2.9999"/>
      <thirdpartyegg name="setuptools" minversion="51.0.0"/> 
      <thirdpartyegg name="pip" minversion="21.1"/> 
      <thirdpartyegg name="toml"/> 
    </deps>
  </version>

  <distpackage type="wheel">
    <packageindex repository="pypi"/>
  </distpackage>
  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>

</project>