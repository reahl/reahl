    <thirdpartyegg name="pytest-runner"/>
  </deps>

  <deps purpose="test">
    <thirdpartyegg name="pytest" minversion="3.0"/>
  </deps>


  <export entrypoint="reahl.component.commands" name="Refresh" locator="reahl.dev.devshell:Refresh"/>
