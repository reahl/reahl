<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>
  
  <deps purpose="test">
    <thirdpartyegg name="pytest" minversion="3.0"/>
  </deps>

  <version number="6.0">
    <deps purpose="run">
      <egg name="reahl-component" versionlocked="True"/>
      <egg name="reahl-tofu" versionlocked="True"/>
      <egg name="reahl-stubble" versionlocked="True"/>
      <thirdpartyegg name="Babel" minversion="2.1" maxversion="2.9.999"/>
      <thirdpartyegg name="twine" minversion="1.15.0" maxversion="3.8.9999"/>
      <thirdpartyegg name="wheel" minversion="0.34.0"/>
      <thirdpartyegg name="tzlocal" minversion="2.0.0" maxversion="4.1.9999"/>
      <thirdpartyegg name="setuptools" minversion="51.0.0"/> 
      <thirdpartyegg name="pip" minversion="10.0.0"/> 
      <thirdpartyegg name="toml"/> 
    </deps>
  </version>

  <distpackage type="wheel">
    <packageindex repository="pypi"/>
  </distpackage>
  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>

</project>