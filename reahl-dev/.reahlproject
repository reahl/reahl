  <export entrypoint="reahl.dev.xmlclasses" name="ExtrasList" locator="reahl.dev.devdomain:ExtrasList"/>

  <export entrypoint="reahl.dev.xmlclasses" name="EntryPointExport" locator="reahl.dev.devdomain:EntryPointExport"/>
