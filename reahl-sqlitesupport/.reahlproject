<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>

  <deps purpose="run">
    <egg name="reahl-component"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    
    <egg name="reahl-stubble"/>
  </deps>

  <export entrypoint="reahl.component.databasecontrols" name="SQLiteControl" locator="reahl.sqlitesupport:SQLiteControl"/>

  <alias name="unit" command="setup -- -q nosetests"/>

</project>
