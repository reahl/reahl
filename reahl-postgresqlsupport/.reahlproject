<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>
  
  <deps purpose="run">
    <egg name="reahl-component"/>
    <thirdpartyegg name="psycopg2" minversion="2.4" maxversion="2.5"/>
  </deps>

  <deps purpose="test">
    <egg name="reahl-tofu"/>
    
    <egg name="reahl-stubble"/>
  </deps>

  <export entrypoint="reahl.component.databasecontrols" name="PostgresqlControl" locator="reahl.postgresqlsupport:PostgresqlControl"/>

  <alias name="unit" command="setup -- -q nosetests"/>
  
  <distpackage type="sdist">
    <packageindex repository="http://pypi.python.org/pypi"/>
  </distpackage>

</project>
