<project type="egg">
  <namespaces>
    <package name="reahl"/>
  </namespaces>

  <deps purpose="build">
    <thirdpartyegg name="setuptools-git" minversion="1.1"/>
    <thirdpartyegg name="pytest-runner"/>
  </deps>

  <deps purpose="test">
    <thirdpartyegg name="pytest" minversion="3.0"/>
  </deps>

  <version number="5.1">
    <deps purpose="run">
      <egg name="reahl-component" versionlocked="True"/>
      <thirdpartyegg name="wrapt" minversion="1.11.0" maxversion="1.12.999"/>
    </deps>
  </version>

  <version number="5.0">
    <deps purpose="run">
      <egg name="reahl-component" versionlocked="True"/>
      <thirdpartyegg name="wrapt" minversion="1.11.0" maxversion="1.12.999"/>
    </deps>
  </version>

  <version number="4.0">
    <deps purpose="run">
      <egg name="reahl-component" versionlocked="True"/>
      <thirdpartyegg name="six"/>
      <thirdpartyegg name="wrapt" minversion="1.10.2" maxversion="1.10.999"/>
    </deps>
  </version>

  <version number="3.2">
    <deps purpose="run">
      <thirdpartyegg name="six"/>
    </deps>
  </version>

  <version number="3.1">
    <deps purpose="run">
      <thirdpartyegg name="six"/>
    </deps>
  </version>

  <version number="3.0">
      <deps purpose="run">
          <egg name="reahl-component" versionlocked="True"/>
          <thirdpartyegg name="nose"/>
      </deps>
  </version>

  <version number="2.1">
      <deps purpose="run">
          <egg name="reahl-component" versionlocked="True"/>
          <thirdpartyegg name="nose"/>
      </deps>
  </version>

  <version number="2.0">
      <deps purpose="run">
          <egg name="reahl-component" versionlocked="True"/>
          <thirdpartyegg name="nose"/>
      </deps>
  </version>


  <distpackage type="wheel">
    <packageindex repository="pypi"/>
  </distpackage>
  <distpackage type="sdist">
    <packageindex repository="pypi"/>
  </distpackage>

</project>

